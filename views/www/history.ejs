<!doctype html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<link rel="icon" type="image/png" href="assets/img/circle_logo.png" />

	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

	<!-- CSS Files -->
    <link href="assets/css/bootstrap.min.css" rel="stylesheet" />
	<link href="assets/css/paper-bootstrap-wizard.css" rel="stylesheet" />


	<link href="assets/css/demo.css" rel="stylesheet" />
	<link href="assets/css/w3.css" rel="stylesheet" />
	<link href="assets/css/animate.css" rel="stylesheet" />

	<!-- Fonts and Icons -->
    <link href="http://netdna.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.css" rel="stylesheet">
	<link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>
	<link href="assets/css/themify-icons.css" rel="stylesheet">
</head>
<body>
	<div class="image-container set-full-height animated slideInRight">
	<div class="container">
		<a class="back-btn" href="home.html">Back</a>
	</div>
	    <div class="container" >
	        <div class="row" style="overflow: scroll;">
		        <div class="col-sm-8 col-sm-offset-2">

		            <div class="wizard-container-logs"  >
		                <div class="card-home wizard-card purchase" data-color="green" id="wizard">
		                <form action="" method="">

		                    	<div class="wizard-header" style=" position: relative;">
		                    		<img src="assets/img/circle_logo.png" class="adb-logo">
		                    	</div>

		                    	<div class="wizard-header" >
	                        		<h3 class="wizard-title">Transaction History</h3>
		                    	</div>

		                    <div style="overflow: scroll; height: 250px; background-color: #f5f6fa">
                    			<div class="col-sm-5 col-sm-offset-2" style="border: solid; border-width: 0px 0px 1px 0px;">
                                	<div class="form-group" >
                                		<!-- You ______ (Amount) __to/from__ _______ -->
                                		<%var i=1; %>
                                		<%for(; i < transactions.length; i++){%>
                                		<label>You</label>
                                		<label>paid</label>   
                                		<label><%=transactions[i].transaction%></label>
                                		<label>to</label> 
                                		<label><%= transactions[i].seller %></label>  
                                		<div style="float: right;"><i><%= transactions[i].date %></i></div>
                                		<%}%>
                                	</div>
                            	</div>
                            </div>
                            	<br>

		                        <div class="wizard-footer">
	                            	<div style="text-align: center;">
	                                    <a href="home2.html"><input type='button' class='btn btn-finish btn-fill btn-wd loginBtn' name='finish' value='Confirm' /></a>
									</div>
	                                <div class="clearfix"></div>
		                        </div>
		                        
		                    </form>
		                </div>
		            </div> <!-- wizard container -->
		        </div>
	        </div> <!-- row -->
	    </div> <!--  big container -->
	</div>
</body>

	<!--   Core JS Files   -->
	<script src="assets/js/jquery-2.2.4.min.js" type="text/javascript"></script>
	<script src="assets/js/bootstrap.min.js" type="text/javascript"></script>
	<script src="assets/js/jquery.bootstrap.wizard.js" type="text/javascript"></script>
	<!--  Plugin for the Wizard -->
	<script src="assets/js/paper-bootstrap-wizard.js" type="text/javascript"></script>
	<!--  More information about jquery.validate here: http://jqueryvalidation.org/	 -->
	<script src="assets/js/jquery.validate.min.js" type="text/javascript"></script>
</html>
